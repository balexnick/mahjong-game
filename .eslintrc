{
  "extends": [
    "eslint:recommended",
    "react-app"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "import", "react-hooks"],
  "rules": {
    "no-console": "warn",
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "react-hooks/exhaustive-deps": "warn",
    "react/destructuring-assignment": ["warn"]
  }
}